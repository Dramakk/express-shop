<%- include('../head') %>
<%- include('admin-navbar') %>
<body class="main-body">
    <main class="container">
        <div class="row text-center" style="align-items: center; margin-top: 10px; margin-bottom: 10px;">
            <div class="col-lg">
                <h2 class="text-center">Lista użytkowników</h2>
                <div class="row">
                    <div class="col-lg">Email</div>
                    <div class="col-lg">Liczba zamówień</div>
                </div>
                <% for(var i=0; i < users.length; i++){ %>
                    <div class="row" style="border: 1px black solid; align-items: center;">
                        <div class="col-lg">
                            <%= users[i].email %>
                        </div>
                        <div class="col-lg"><%= users[i].count %></div>
                    </div>
                <% } %>
            </div>
    </main>
</body>